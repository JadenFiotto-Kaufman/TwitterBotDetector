{
  "name": "Tweetect",
  "version": "0.1",
  "manifest_version": 2,
  "description": "TwitterBotDetector",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
        ],
      "css":["contentCSS.css"],
      "js": ["jquery-3.3.1.min.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "",
    "default_icon": "icon.png"
  }
}